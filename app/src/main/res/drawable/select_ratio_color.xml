<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Unchecked state -->
    <item android:state_checked="false"
         android:drawable="@drawable/unchecked_circle"/>

    <!-- Checked state -->
    <item android:state_checked="true" android:drawable="@drawable/checked_circle" />

</selector>
